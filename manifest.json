{
    "content_scripts": [
        {
            "matches": ["https://www.pivotaltracker.com/*"],
            "js": ["git-button.js"]
        }
    ],
    "name": "Git Button",
    "description": "Add a copy button to pivotal so you can paste a git command that creates a new branch.",
    "homepage_url": "http://git-button.joeldare.com",
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png" 
    },
    "permissions": [
        "https://www.pivotaltracker.com/*"
    ],
    "version": "16.12.14",
    "manifest_version": 2
}
